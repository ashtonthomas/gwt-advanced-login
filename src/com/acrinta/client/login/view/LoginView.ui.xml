<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>

	</ui:style>
	<g:HTMLPanel>
		<h1>Advanced Login</h1>
		<g:SimplePanel ui:field="message"></g:SimplePanel>
		<table>
			<tr>
				<td>Email</td>
				<td>
					<g:TextBox ui:field="email"></g:TextBox>
				</td>
			</tr>
			<tr>
				<td>Password</td>
				<td>
					<g:PasswordTextBox ui:field="password"></g:PasswordTextBox>
				</td>
			</tr>
			<tr>
				<td></td>
				<td>
					<g:Button ui:field="submit_button"></g:Button>
					<g:Anchor ui:field="register_link"></g:Anchor>
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 